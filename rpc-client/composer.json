{
    "name": "kuiper/rpc-client",
    "type": "library",
    "description": "kuiper rpc client component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ocramius/proxy-manager": "^1.0",
        "symfony/event-dispatcher": "^3.1",
        "kuiper/rpc": "^0.3",
        "kuiper/serializer": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For use http json client (version ^6.2)"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\rpc\\client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\rpc\\client\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3-dev"
        }
    }
}
