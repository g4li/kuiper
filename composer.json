{
    "name": "kuiper/kuiper",
    "type": "library",
    "description": "The Kuiper PHP framework",
    "keywords": ["framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0",
        "symfony/event-dispatcher": "^5.0"
    },
    "require-dev": {
        "aura/sqlquery": "^2.7",
        "dflydev/fig-cookies": "^2.0",
        "guzzlehttp/guzzle": "^6.2",
        "laminas/laminas-diactoros": "^2.2",
        "mockery/mockery": "^1.3",
        "monolog/monolog": "^2.0",
        "nesbot/carbon": "^2.34",
        "nikic/fast-route": "^1.0",
        "php-di/php-di": "^6.0",
        "phpunit/phpunit": "^8.0",
        "slim/slim": "^4.5",
        "swoft/swoole-ide-helper": "^4.4",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^4.1",
        "winwin/composer-phpcs-plugin": "^0.1"
    },
    "suggest": {
        "nikic/fast-route": "Required to use web component",
        "dflydev/fig-cookies": "Required to use web component",
        "twig/twig": "Required to use twig template"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\annotations\\": "annotations/src/",
            "kuiper\\db\\": "db/src/",
            "kuiper\\di\\": "di/src/",
            "kuiper\\helper\\": "helper/src/",
            "kuiper\\reflection\\": "reflection/src/",
            "kuiper\\swoole\\": "swoole/src/",
            "kuiper\\event\\": "event/src/",
            "kuiper\\web\\": "web/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\db\\": "db/tests/",
            "kuiper\\di\\": "di/tests/",
            "kuiper\\helper\\": "helper/tests/",
            "kuiper\\reflection\\": "reflection/tests/",
            "kuiper\\swoole\\": "swoole/tests/",
            "kuiper\\web\\": "web/tests/"
        }
    }
}
