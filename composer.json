{
    "name": "kuiper/reflection",
    "type": "library",
    "description": "Php reflection utility",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@163.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
        "kuiper/test-helper": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\reflection\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1-dev"
        }
    }
}
