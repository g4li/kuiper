{
    "name": "kuiper/web",
    "type": "library",
    "description": "kuiper web component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "psr/cache": "^1.0",
        "dflydev/fig-cookies": "^1.0",
        "nikic/fast-route": "^1.0"
    },
    "require-dev": {
        "zendframework/zend-diactoros": "^1.3",
        "kuiper/test-helper": "^0.1",
        "kuiper/annotations": "^0.1",
        "kuiper/helper": "^0.1",
        "kuiper/cache": "^0.1",
        "kuiper/di": "^0.1"
    },
    "suggest": {
        "kuiper/annotations": "for filter middleware annotations and route definition"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\web\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\web\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1-dev"
        }
    }
}
