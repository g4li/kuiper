{
    "name": "kuiper/cache",
    "type": "library",
    "description": "Cache component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "psr/cache": "^1.0"
    },
    "suggest": {
        "ext-redis": "For use redis cache",
        "ext-apcu": "For use apcu cache"
    },
    "require-dev": {
        "winwin/composer-phpcs-plugin": "^0.1",
        "phpunit/phpunit": "^5.0",
        "vlucas/phpdotenv": "^2.4",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\cache\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
