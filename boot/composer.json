{
    "name": "kuiper/boot",
    "type": "library",
    "description": "kuiper boot component",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Ye Wenbin",
            "email": "wenbinye@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "kuiper/di": "~0.2",
        "kuiper/helper": "~0.2",
        "kuiper/web": "~0.2",
        "kuiper/reflection": "~0.2",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "twig/twig": "^1.7",
        "kuiper/annotations": "~0.2",
        "kuiper/cache": "~0.2",
        "monolog/monolog": "^1.22",
        "winwin/composer-phpcs-plugin": "^0.1.0"
    },
    "suggest": {
        "kuiper/annotations": "for filter middleware annotations and route definition",
        "ocramius/proxy-manager": "for profile annotation and lazy object"
    },
    "autoload": {
        "psr-4": {
            "kuiper\\boot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kuiper\\boot\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3-dev"
        }
    }
}
